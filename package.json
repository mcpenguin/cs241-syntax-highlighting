{
    "name": "cs241-syntax-highlighting",
    "displayName": "CS241 Syntax Highlighting",
    "description": "Syntax highlighting for CS241",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.63.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "publisher": "mcpenguin",
    "repository": "https://github.com/mcpenguin/cs241-syntax-highlighting",
    "contributes": {
        "languages": [
            {
                "id": "asm",
                "aliases": [
                    "CS241 asm",
                    "asm"
                ],
                "extensions": [
                    ".asm"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "asm",
                "scopeName": "source.asm",
                "path": "./syntaxes/asm.tmLanguage.json"
            }
        ]
    },
    "devDependencies": {
        "js-yaml": "^4.1.0"
    },
    "scripts": {
        "build": "npx js-yaml syntaxes/asm.tmLanguage.yaml > syntaxes/asm.tmLanguage.json"
    }
}
