{
    "name": "cs241-syntax-highlighting",
    "displayName": "CS241 Syntax Highlighting",
    "description": "Syntax highlighting for CS241",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.63.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "publisher": "mcpenguin",
    "repository": "https://github.com/mcpenguin/cs241-syntax-highlighting",
    "contributes": {
        "languages": [
            {
                "id": "hex",
                "aliases": [
                    "CS241 Hex",
                    "hex"
                ],
                "extensions": [
                    ".hex"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "hex",
                "scopeName": "source.hex",
                "path": "./syntaxes/hex.tmLanguage.json"
            }
        ]
    },
    "devDependencies": {
        "js-yaml": "^4.1.0"
    },
    "scripts": {
        "build": "npx js-yaml syntaxes/hex.tmLanguage.yaml > syntaxes/hex.tmLanguage.json"
    }
}
