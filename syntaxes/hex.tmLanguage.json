{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "CS241 Hex",
  "patterns": [
    {
      "name": "hex.comment",
      "begin": ";",
      "end": "\\n"
    },
    {
      "name": "hex.register",
      "match": "\\$([0-9])+"
    },
    {
      "name": "hex.literal",
      "match": "lis|\\.word"
    },
    {
      "name": "hex.operation",
      "match": "add|sub|mult|multu|div|divu"
    },
    {
      "name": "hex.loadstore",
      "match": "lw|sw"
    },
    {
      "name": "hex.setless",
      "match": "slt|sltu"
    },
    {
      "name": "hex.branch",
      "match": "beq|bne"
    },
    {
      "name": "hex.jump",
      "match": "jr|jalr"
    },
    {
      "name": "hex.constant",
      "match": "[0-9]+|-[0-9]+"
    },
    {
      "name": "hex.label",
      "match": "\\w+"
    }
  ],
  "scopeName": "source.hex"
}
